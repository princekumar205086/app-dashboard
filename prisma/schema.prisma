datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}


enum Role {
  USER
  ADMIN
}

model User {
  id       Int      @id @default(autoincrement())
  username String
  email    String   @unique
  password String
  role     Role     @default(USER)
}
